{
  "name": "@lerna-lite/changed",
  "description": "Lerna-Lite changed commmand - List local packages that have changed since the last tagged release",
  "version": "4.5.1",
  "files": [
    "/dist"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js"
  },
  "scripts": {
    "prebuild": "remove dist",
    "build": "tsc",
    "build:incremental": "tsc --incremental --sourceMap false --excludeDirectories dist",
    "echo:test": "echo \"Lerna-Lite changed package\"",
    "pack-tarball": "npm pack"
  },
  "license": "MIT",
  "author": "Ghislain B.",
  "homepage": "https://github.com/lerna-lite/lerna-lite",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lerna-lite/lerna-lite.git",
    "directory": "packages/changed"
  },
  "bugs": {
    "url": "https://github.com/lerna-lite/lerna-lite/issues"
  },
  "engines": {
    "node": "^20.17.0 || >=22.9.0"
  },
  "dependencies": {
    "@lerna-lite/cli": "workspace:*",
    "@lerna-lite/core": "workspace:*",
    "@lerna-lite/list": "workspace:*",
    "@lerna-lite/listable": "workspace:*"
  },
  "devDependencies": {
    "yargs-parser": "catalog:"
  }
}
