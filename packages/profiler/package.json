{
  "name": "@lerna-lite/profiler",
  "description": "Lerna-Lite Profiler used by certain optional commands (Run/Exec)",
  "version": "4.7.0",
  "files": [
    "/dist"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js"
  },
  "scripts": {
    "prebuild": "remove dist",
    "build": "tsc",
    "build:incremental": "tsc --incremental --sourceMap false --excludeDirectories dist",
    "echo:test": "echo \"Lerna-Lite profiler package\"",
    "pack-tarball": "npm pack"
  },
  "license": "MIT",
  "author": "Ghislain B.",
  "homepage": "https://github.com/lerna-lite/lerna-lite",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lerna-lite/lerna-lite.git",
    "directory": "packages/profiler"
  },
  "bugs": {
    "url": "https://github.com/lerna-lite/lerna-lite/issues"
  },
  "engines": {
    "node": "^20.17.0 || >=22.9.0"
  },
  "dependencies": {
    "@lerna-lite/core": "workspace:*",
    "@lerna-lite/npmlog": "workspace:*",
    "fs-extra": "catalog:",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "catalog:devDependencies",
    "yargs": "catalog:dependencies"
  }
}
