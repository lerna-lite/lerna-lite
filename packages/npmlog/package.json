{
  "name": "@lerna-lite/npmlog",
  "description": "Lerna-Lite npmlog reimplementation of the now deprecated npm/npmlog",
  "version": "4.7.3",
  "files": [
    "/dist"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js"
  },
  "scripts": {
    "prebuild": "remove dist",
    "build": "tsc",
    "build:incremental": "tsc --incremental --sourceMap false --excludeDirectories dist",
    "echo:test": "echo \"Lerna-Lite npmlog package\"",
    "pack-tarball": "npm pack"
  },
  "license": "MIT",
  "author": "Ghislain B.",
  "homepage": "https://github.com/lerna-lite/lerna-lite",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lerna-lite/lerna-lite.git",
    "directory": "packages/npmlog"
  },
  "bugs": {
    "url": "https://github.com/lerna-lite/lerna-lite/issues"
  },
  "engines": {
    "node": "^20.17.0 || >=22.9.0"
  },
  "dependencies": {
    "aproba": "^2.1.0",
    "fast-string-width": "^3.0.2",
    "has-unicode": "catalog:",
    "set-blocking": "^2.0.0",
    "signal-exit": "^4.1.0",
    "tinyrainbow": "catalog:dependencies",
    "wide-align": "^1.1.5"
  }
}
